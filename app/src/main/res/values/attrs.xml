<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SupperButton">
        <!-- normal-->
        <!-- 文字颜色 -->
        <attr name="normalTextColor" format="color" />
        <!-- drawable -->
        <attr name="normalSolidColor" format="color" />
        <attr name="normalStrokeColor" format="color" />
        <attr name="normalStrokeWidth" format="dimension" />
        <attr name="normalStrokeDashWidth" format="dimension" />
        <attr name="normalStrokeDashGap" format="dimension" />
        <attr name="normalRadius" format="dimension" />
        <attr name="normalTopLeftRadius" format="dimension" />
        <attr name="normalTopRightRadius" format="dimension" />
        <attr name="normalBottomLeftRadius" format="dimension" />
        <attr name="normalBottomRightRadius" format="dimension" />
        <attr name="normalGradientStartColor" format="color" />
        <attr name="normalGradientEndColor" format="color" />
        <attr name="normalGradientCenterColor" format="color" />
        <attr name="normalGradientUseLevel" format="boolean" />
        <attr name="normalGradientAngle" format="dimension" />
        <attr name="normalGradientRadius" format="dimension" />
        <attr name="normalGradientOrientation">
            <enum name="BL_TR" value="0" />
            <enum name="BOTTOM_TOP" value="1" />
            <enum name="BR_TL" value="2" />
            <enum name="LEFT_RIGHT" value="3" />
            <enum name="RIGHT_LEFT" value="4" />
            <enum name="TL_BR" value="5" />
            <enum name="TOP_BOTTOM" value="6" />
            <enum name="TR_BL" value="7" />
        </attr>
        <attr name="normalShapeType">
            <enum name="LINEAR_GRADIENT" value="0" />
            <enum name="OVAL" value="1" />
            <enum name="LINE" value="2" />
            <enum name="RING" value="3" />
        </attr>
        <attr name="normalGradientType">
            <enum name="linear" value="0" />
            <enum name="radial" value="1" />
            <enum name="sweep" value="2" />
        </attr>

